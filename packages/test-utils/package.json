{
  "name": "@tcwatch/test-utils",
  "version": "1.0.0",
  "description": "Shared testing utilities for TCWatch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@supabase/supabase-js": "^2.38.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.17",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "msw": "^2.0.11",
    "supertest": "^6.3.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./backend": {
      "import": "./dist/backend.js",
      "types": "./dist/backend.d.ts"
    },
    "./frontend": {
      "import": "./dist/frontend.js",
      "types": "./dist/frontend.d.ts"
    },
    "./mocks": {
      "import": "./dist/mocks.js",
      "types": "./dist/mocks.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tcwatch.git",
    "directory": "packages/test-utils"
  },
  "license": "MIT",
  "keywords": [
    "tcwatch",
    "testing",
    "utilities",
    "jest",
    "test-utils"
  ]
}