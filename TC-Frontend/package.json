{
  "name": "tcwatch-frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "build": "expo export",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:ci": "jest --coverage --watchAll=false --maxWorkers=2",
    "test:search": "jest --testNamePattern='search|Search'",
    "test:components": "jest --testPathPatterns=components",
    "test:hooks": "jest --testPathPatterns=hooks",
    "test:screens": "jest --testPathPatterns=screens",
    "test:accessibility": "jest --testNamePattern='a11y|accessibility|Accessibility'",
    "test:performance": "jest --testPathPatterns=performance",
    "test:snapshot": "jest --testNamePattern='snapshot'",
    "test:e2e": "jest --testPathPatterns=e2e",
    "test:phase5": "jest --testPathPatterns='search|home|content|personalization'",
    "test:failing": "jest --onlyFailures",
    "test:silent": "jest --silent",
    "test:verbose": "jest --verbose",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:clear-cache": "jest --clearCache"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.56.2",
    "@trpc/client": "^11.5.1",
    "@trpc/react-query": "^11.5.1",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expo": "~54.0.13",
    "expo-apple-authentication": "^8.0.7",
    "expo-auth-session": "^7.0.8",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-document-picker": "^14.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "^17.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.12",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.17",
    "uuid": "^13.0.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/methods": "^1.1.4",
    "@types/ms": "^2.1.0",
    "@types/normalize-package-data": "^2.4.4",
    "@types/phoenix": "^1.6.6",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.0.0",
    "@types/semver": "^7.7.1",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "@types/ws": "^8.18.1",
    "eslint": "^8.54.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.12",
    "msw": "^2.11.3",
    "prettier": "^3.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.9.2"
  },
  "private": true,
  "jest": {
    "preset": "./jest-preset.js"
  }
}
